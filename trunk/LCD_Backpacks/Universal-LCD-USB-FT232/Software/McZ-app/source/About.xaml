<Window x:Class="USB_LCD_Backpack.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="About USB LCD Backpack Test Program" Height="300" Width="300" ResizeMode="NoResize" Name="AboutUSBLCDBackpack">
    <Grid>
        <Label Margin="80,20,0,0" Name="Version" Height="30"
               VerticalAlignment="Top" HorizontalAlignment="Left"
               Width="75" Content="Version 0.03"></Label>
        <Label Margin="80,50,0,0" Name="buildDate" Height="30"
               VerticalAlignment="Top" HorizontalAlignment="Left"
               Width="160">Build date: Sept 2010</Label>
        <Label Margin="0,95,0,0" Name="text" Height="28"
               VerticalAlignment="Top" HorizontalAlignment="Left"
               Width="278">Test program to support USB LCD Backpack PCA. </Label>
        <Label Margin="49,169,0,0" Name="text1" Height="32"
               VerticalAlignment="Top" HorizontalAlignment="Left"
               Width="191">http://dangerousprototypes.com/</Label>
        <Button Margin="98,217,0,0" Name="ok" HorizontalAlignment="Left" Width="78"
                Height="23" VerticalAlignment="Top" Click="ok_Click">OK</Button>
        <GridSplitter Height="2" HorizontalAlignment="Left" Margin="12,78,0,0" Name="gridSplitter1" VerticalAlignment="Top" Width="254" />
    </Grid>
</Window>
